    "date-fns": "^3.6.0",
    "emoji-mart": "^5.6.0",
    "framer-motion": "^11.2.13",
    "i18next": "^23.12.2",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.5.2",
